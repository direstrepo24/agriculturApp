{
  "components": [
    {
      "type": "row",
      "components": [
        {
          "type": "col",
          "class": "col-md-12",
          "components": [
            {
              "data": "",
              "events": {
                "cancel": [
                  {
                    "type": "back"
                  }
                ],
                "submit": [
                  {
                    "error": [
                      {
                        "detail": "Unable to create new Insumo!",
                        "severity": "error",
                        "summary": "Error",
                        "type": "notify"
                      }
                    ],
                    "name": "agriculturebd.createInsumo",
                    "parameters": [
                      {
                        "name": "Insumo",
                        "value": "${event}"
                      }
                    ],
                    "then": [
                      {
                        "path": "insumos",
                        "type": "navigate"
                      }
                    ],
                    "type": "invoke"
                  }
                ]
              },
              "fields": [
                {
                  "property": "Descripcion",
                  "required": false,
                  "title": "Descripcion",
                  "type": "string",
                  "requiredText": "is required"
                },
                {
                  "property": "Fecha_Vencimiento",
                  "required": true,
                  "title": "Fecha Vencimiento",
                  "type": "date",
                  "requiredText": "is required"
                },
                {
                  "property": "Id",
                  "required": true,
                  "title": "Id",
                  "type": "integer",
                  "requiredText": "is required"
                },
                {
                  "property": "Nombre",
                  "required": false,
                  "title": "Nombre",
                  "type": "string",
                  "requiredText": "is required"
                },
                {
                  "property": "lote_laboratorio",
                  "required": false,
                  "title": "Lote Laboratorio",
                  "type": "string",
                  "requiredText": "is required"
                },
                {
                  "data": "${getTipoInsumosResult}",
                  "property": "Tipo_InsumoId",
                  "required": true,
                  "textProperty": "Nombre",
                  "title": "Tipo Insumo",
                  "type": "lookup",
                  "valueProperty": "Id",
                  "placeholder": "Choose TipoInsumo",
                  "requiredText": "is required"
                },
                {
                  "data": "${getLaboratoriosResult}",
                  "property": "laboratorioId",
                  "required": true,
                  "textProperty": "Nombre",
                  "title": "Laboratorio",
                  "type": "lookup",
                  "valueProperty": "Id",
                  "placeholder": "Choose Laboratorio",
                  "requiredText": "is required"
                }
              ],
              "name": "form0",
              "type": "form"
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "agriculturebd.getTipoInsumos",
        "parameters": [],
        "then": [
          {
            "name": "getTipoInsumosResult",
            "type": "set",
            "value": "${result.value}"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "agriculturebd.getLaboratorios",
        "parameters": [],
        "then": [
          {
            "name": "getLaboratoriosResult",
            "type": "set",
            "value": "${result.value}"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Add Insumo",
  "access": [
    "Authenticated"
  ]
}