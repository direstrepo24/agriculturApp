{
  "components": [
    {
      "type": "row",
      "components": [
        {
          "type": "col",
          "class": "col-md-12",
          "components": [
            {
              "data": "",
              "events": {
                "cancel": [
                  {
                    "type": "back"
                  }
                ],
                "submit": [
                  {
                    "error": [
                      {
                        "detail": "Unable to create new Foto!",
                        "severity": "error",
                        "summary": "Error",
                        "type": "notify"
                      }
                    ],
                    "name": "agriculturebd.createFoto",
                    "parameters": [
                      {
                        "name": "Foto",
                        "value": "${event}"
                      }
                    ],
                    "then": [
                      {
                        "path": "fotos",
                        "type": "navigate"
                      }
                    ],
                    "type": "invoke"
                  }
                ]
              },
              "fields": [
                {
                  "property": "Descripcion",
                  "required": false,
                  "title": "Descripcion",
                  "type": "string",
                  "requiredText": "is required"
                },
                {
                  "property": "FechaCreacion",
                  "required": true,
                  "title": "Fecha Creacion",
                  "type": "date",
                  "requiredText": "is required"
                },
                {
                  "property": "Hora",
                  "required": false,
                  "title": "Hora",
                  "type": "string",
                  "requiredText": "is required"
                },
                {
                  "property": "Id",
                  "required": true,
                  "title": "Id",
                  "type": "integer",
                  "requiredText": "is required"
                },
                {
                  "property": "Ruta",
                  "required": false,
                  "title": "Ruta",
                  "type": "string",
                  "requiredText": "is required"
                },
                {
                  "property": "Titulo",
                  "required": false,
                  "title": "Titulo",
                  "type": "string",
                  "requiredText": "is required"
                },
                {
                  "data": "${getEnfermedadesResult}",
                  "property": "EnfermedadesId",
                  "required": true,
                  "textProperty": "Codigo",
                  "title": "Enfermedade",
                  "type": "lookup",
                  "valueProperty": "Id",
                  "placeholder": "Choose Enfermedade",
                  "requiredText": "is required"
                }
              ],
              "name": "form0",
              "type": "form"
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "agriculturebd.getEnfermedades",
        "parameters": [],
        "then": [
          {
            "name": "getEnfermedadesResult",
            "type": "set",
            "value": "${result.value}"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Add Foto",
  "access": [
    "Authenticated"
  ]
}